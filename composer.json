{
    "name": "payum/payum-bundle",
    "type": "symfony-bundle",
    "description": "Integrate payum libs into symfony2 framework",
    "keywords": ["payment", "paypal", "be2bill"],
    "homepage": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Kotlyar Maksim",
            "email": "kotlyar.maksim@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "payum/payum": "0.1.*",
        "symfony/framework-bundle": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "payum/paypal-express-checkout-nvp": "0.1.*",
        "payum/be2bill": "0.1.*",
        "payum/authorize-net-aim": "0.1.*",
        "ajbdev/authorizenet-php-api": "dev-master@dev"
    },
    "suggest": {
        "payum/paypal-express-checkout-nvp": "If you want to use paypal express checkout nvp gateway",
        "payum/authorize-net-aim": "If you want to use authorize.net gateway",
        "payum/be2bill": "If you want to use be2bill gateway"
    },
    "autoload": {
        "psr-0": { "Payum\\Bundle\\PayumBundle": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "target-dir": "Payum/Bundle/PayumBundle"
}