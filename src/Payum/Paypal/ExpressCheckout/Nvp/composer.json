{
    "name": "payum/paypal-express-checkout-nvp",
    "type": "library",
    "description": "The Payum extension. It provides Paypal ExpressCheckout payment integration.",
    "keywords": ["payment", "paypal", "express checkout", "digital goods", "recurring payment"],
    "homepage": "https://payum.forma-pro.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Kotlyar Maksim",
            "email": "kotlyar.maksim@gmail.com"
        },
        {
            "name":     "Payum project",
            "homepage": "https://payum.forma-pro.com/"
        },
        {
            "name":     "Community contributions",
            "homepage": "https://github.com/Payum/PaypalExpressCheckoutNvp/contributors"
        }
    ],
    "require": {
        "payum/core": "^1.6"
    },
    "require-dev": {
        "payum/core": "^1.6",
        "phpunit/phpunit": "^5.7",
        "php-http/guzzle6-adapter": "^1.0",
        "symfony/phpunit-bridge": "^3.1|^4"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Payum\\Paypal\\ExpressCheckout\\Nvp": "" }
    },
    "target-dir": "Payum/Paypal/ExpressCheckout/Nvp",
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    }
}
