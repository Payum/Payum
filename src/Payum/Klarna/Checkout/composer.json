{
    "name": "payum/klarna-checkout",
    "type": "library",
    "description": "The Payum extension. It provides Klarna Checkout payment integration.",
    "keywords": [
        "payment",
        "klarna"
    ],
    "homepage": "https://payum.forma-pro.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Kotlyar Maksim",
            "email": "kotlyar.maksim@gmail.com"
        },
        {
            "name":     "Payum project",
            "homepage": "https://payum.forma-pro.com/"
        },
        {
            "name":     "Community contributions",
            "homepage": "https://github.com/Payum/KlarnaCheckout/contributors"
        }
    ],
    "require": {
        "payum/core": "^1.3",
        "klarna/checkout": "^1|^2"
    },
    "require-dev": {
        "payum/core": "^1.3@dev",
        "phpunit/phpunit": "~4.0",
        "symfony/phpunit-bridge": "~2.8|~3.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Payum\\Klarna\\Checkout": "" }
    },
    "target-dir": "Payum/Klarna/Checkout",
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    }
}
